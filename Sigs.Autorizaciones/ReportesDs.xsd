                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select	a.Id Numero,
		ta.Nombre TipoAutorizacion,
		a.FechaAutorizacion,
		a.FechaServicio,
		t.Nombre TipoPrestadora,
		p.Nombre Prestadora,
		af.Nombres + ' ' + isnull(af.Apellidos, '') Afiliado,
		af.NSS,
		af.Sexo,
		datediff(year, af.FechaNacimiento, a.FechaServicio) Edad,
		cb.SIMON Codigo,
		cb.Nombre Cobertura,
		pa.Cantidad,
		pa.Tarifa Solicitado,		
		pa.Aprobado,
		a.RulesAppliances ReglasAutorizacion,
		pa.RulesAppliances ReglasPrestacion
from	dbo.Autorizaciones a
		inner join dbo.Afiliado af
		on a.AfiliadoId = af.Id
		inner join prestadoras p
		on a.PrestadoraId = p.Id
		inner join dbo.TiposPrestadoras t
		on p.TipoPrestadoraId = t.Id
		inner join dbo.PrestacionesAutorizaciones pa
		on a.Id = pa.AutorizacionId
		inner join dbo.Prestaciones pr
		on pa.PrestacionId = pr.Id
		inner join dbo.Coberturas cb
		on pr.CoberturaId = cb.Id
		inner join dbo.TiposAutorizaciones ta
		on a.TipoAutorizacionId = ta.Id
where a.Id = @autorizacionId</CommandText>
                    <Parameters>
